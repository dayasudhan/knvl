generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model People {
  id        Int       @id @default(autoincrement())
  firstName String
  lastName  String
  email     String
  medical   Json?

  @@map("people")
}
model Users {
  id        Int        @id @default(autoincrement())
  username  String?    
  password  String?    
  role      String?    
  password2 String?    

  @@map("users")
}
