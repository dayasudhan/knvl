      <div class="deladdresspart">
            <ul class="deladdresslist">
                <li ng-repeat="address in profile.addresses">
                    <div class="deladdlist">
                        <h3><input type="radio" 
                            name="name" required
                            ng-click="fnChangeAssetType(address)" > &nbsp; {{address.label}}</h3>
                    </div>
                    <div class="deladdlistt">
                        <ul class="deladddetails">
                            <li>
                                <p><span>Address Line 1 :</span> {{address.addressLine1}}</p>
                            </li>
                            <li>
                                <p><span>Address Line 2 :</span> {{address.addressLine2}}</p>
                            </li>
                              <li>
                                <p><span>Landmark       :</span> {{address.Landmark}}</p>
                            </li>
                            <li>
                                <p><span>City           :</span> {{address.city}}</p>
                            </li>
                            <li>
                                <p><span>Zip            :</span> {{address.zip}}</p>
                            </li>
                        </ul>
                    </div>
                </li>
            </ul>
        </div>
